{
  "name": "ussf-portal-cms",
  "version": "1.19.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-cms"
  },
  "scripts": {
    "services:removeall": "docker ps --quiet --all | xargs docker rm --force",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "dev": "node -r ./startup/index.js node_modules/.bin/keystone dev",
    "start": "node -r ./startup/index.js node_modules/.bin/keystone start",
    "build": "keystone build",
    "postinstall": "keystone postinstall",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch --runInBand --testTimeout=60000"
  },
  "dependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-typescript": "7.23.3",
    "@graphql-tools/schema": "9.0.19",
    "@k6-contrib/list-plugins": "3.0.0",
    "@keystone-6/auth": "5.0.1",
    "@keystone-6/core": "3.1.3",
    "@keystone-6/fields-document": "8.0.0",
    "@keystone-ui/button": "7.0.2",
    "@prisma/client": "4.3.1",
    "cookie-signature": "1.2.1",
    "dotenv": "16.3.2",
    "luxon": "3.4.4",
    "pino": "8.17.2",
    "react": "18.2.0",
    "redis": "4.6.12",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/preset-react": "7.23.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/cookie": "0.6.0",
    "@types/cookie-signature": "1.1.2",
    "@types/jest": "29.5.11",
    "@types/luxon": "3.4.2",
    "@types/react": "18.2.48",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "2.1.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "13.3.0",
    "prettier": "3.2.5"
  },
  "resolutions": {
    "qs": "6.11.2",
    "undici": "5.28.2",
    "tsconfig-paths": "4.2.0",
    "jsonwebtoken": "9.0.2",
    "fast-xml-parser": "4.2.5",
    "xml2js": "0.6.2"
  },
  "engines": {
    "node": "^18.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  }
}
