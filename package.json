{
  "name": "ussf-portal-cms",
  "version": "1.10.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-cms"
  },
  "scripts": {
    "services:removeall": "docker ps --quiet --all | xargs docker rm --force",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "dev": "node -r ./startup/index.js node_modules/.bin/keystone dev",
    "start": "node -r ./startup/index.js node_modules/.bin/keystone start",
    "build": "keystone build",
    "postinstall": "keystone postinstall",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch --runInBand --testTimeout=60000"
  },
  "dependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.22.5",
    "@graphql-tools/schema": "9.0.19",
    "@k6-contrib/list-plugins": "3.0.0",
    "@keystone-6/auth": "5.0.1",
    "@keystone-6/core": "3.1.3",
    "@keystone-6/fields-document": "8.0.0",
    "@keystone-ui/button": "7.0.2",
    "@prisma/client": "4.3.1",
    "cookie-signature": "1.2.1",
    "dotenv": "16.3.1",
    "luxon": "3.4.0",
    "pino": "8.15.0",
    "react": "18.2.0",
    "redis": "4.6.7",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/preset-react": "7.22.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/cookie": "0.5.1",
    "@types/cookie-signature": "1.1.0",
    "@types/jest": "29.5.3",
    "@types/luxon": "3.3.1",
    "@types/react": "18.2.20",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "29.6.3",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "jest": "29.6.3",
    "jest-environment-jsdom": "29.6.3",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8"
  },
  "resolutions": {
    "qs": "6.11.2",
    "undici": "5.23.0",
    "tsconfig-paths": "4.2.0",
    "jsonwebtoken": "9.0.1",
    "fast-xml-parser": "4.2.4",
    "xml2js": "0.6.2"
  },
  "engines": {
    "node": "^18.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  }
}
